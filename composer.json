{
    "name": "robots/cleaning-robot",
    "description": "Cleaning robot",
    "authors": [
        {
            "name": "Olegs Veramejs"
        }
    ],
    "require": {
        "php": "~7.3",
        "illuminate/container": "5.8.*",
        "illuminate/validation": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "fzaninotto/faker": "^1.8",
        "friendsofphp/php-cs-fixer": "^2.15",
        "sebastian/phpcpd": "^5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Robot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robot\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always --coverage-text",
        "check-cs": "vendor/bin/phpcs",
        "check-cp": "vendor/bin/phpcpd src",
        "fix-cs": "vendor/bin/phpcbf -p"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    }
}
